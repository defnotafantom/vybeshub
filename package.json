{
  "name": "vybes-webapp",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://defnotafantom.github.io/vybes-webapp",
  "scripts": {
    "dev": "vite --port 5173 --strictPort",
    "build": "vite build",
    "preview": "vite preview",
    "stop": "for /f \"tokens=5\" %a in ('netstat -aon ^| findstr :5173') do taskkill /PID %a /F",
    "dev:ngrok": "concurrently \"vite --port 5173 --strictPort\" \"ngrok http 5173 --log=stdout\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^7.3.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/supabase-js": "^2.30.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.0",
    "d3-geo": "^3.1.1",
    "emoji-picker": "^0.0.7",
    "emoji-picker-react": "^4.15.1",
    "framer-motion": "^10.16.4",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "lucide-react": "^0.292.0",
    "maplibre-gl": "^5.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^4.2.1",
    "react-map-gl": "^8.1.0",
    "react-masonry-css": "^1.0.16",
    "react-mentions": "^4.4.10",
    "react-router-dom": "^6.16.0",
    "react-simple-maps": "^3.0.0",
    "react-zoom-pan-pinch": "^3.7.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.6",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/node": "^20.8.3",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.5"
  }
