{
  "name": "vybes-webapp",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://defnotafantom.github.io/vybes-webapp",
  "scripts": {
    "dev": "vite --port 5173 --strictPort",
    "build": "vite build",
    "preview": "vite preview",
    "stop": "for /f \"tokens=5\" %a in ('netstat -aon ^| findstr :5173') do taskkill /PID %a /F",
    "dev:ngrok": "concurrently \"vite --port 5173 --strictPort\" \"ngrok http 5173 --log=stdout\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/supabase-js": "2.30.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.16.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "concurrently": "^8.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.5",
    "gh-pages": "^5.0.0"
  }
}

  
